{
  "manifest_version": 3,
  "name": "The Economist Dark",
  "version": "2.1.0",
  "description": "Read economist.com in dark mode",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "dark.css"
      ],
      "matches": [
        "https://economist.com/*",
        "https://www.economist.com/*"
      ],
      "exclude_matches": [
        "https://economist.com/interactive/*",
        "https://www.economist.com/interactive/*",
        "https://www.economist.com/insider/*"
      ],
      "run_at": "document_start"
    }
  ]
}