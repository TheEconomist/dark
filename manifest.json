{
  "manifest_version": 3,
  "name": "Econ Dark Theme",
  "version": "2.0.0",
  "description": "Read economist.com in dark theme",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "dark.css"
      ],
      "matches": [
        "https://economist.com/*",
        "https://www.economist.com/*"
      ],
      "exclude_matches": [
        "https://economist.com/interactive/*",
        "https://www.economist.com/interactive/*"
      ],
      "run_at": "document_start"
    }
  ]
}